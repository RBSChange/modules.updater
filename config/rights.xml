<?xml version="1.0" encoding="UTF-8"?>
<rights>
  <actions>
    <document name="rootfolder"/>
    <action name="UpdateLankPack" />
    <action name="ApplyHotFix" />
    <action name="ApplyPatch" />
    <action name="DownloadUpgrade" />
  </actions>
  <roles>
    <role name="Admin">
      <permission name="*"/>
      <permission name="UpdateLankPack"/>
      <permission name="ApplyHotFix"/>
      <permission name="ApplyPatch"/>
